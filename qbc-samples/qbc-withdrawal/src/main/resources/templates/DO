package com.qbc.dao;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.PrePersist;
import javax.persistence.PreUpdate;
import javax.persistence.Table;

import lombok.Data;

/**
 * ${remarks}实体类
 *
 * @author Ma
 */
@Data
@Entity
@Table(name = "${tableName}")
public class ${className}DO {

	<#list columnInfos as columnInfo>
	<#if columnInfo_index == 0>
	@Id
	</#if> 
	private ${columnInfo.fieldType} ${columnInfo.fieldName};

	</#list>
	@PrePersist
	public void prePersist() {
		createdBy = lastModifiedBy = "SYSTEM";
		createdDateTime = lastModifiedDateTime = java.time.LocalDateTime.now();
	}

	@PreUpdate
	public void preUpdate() {
		lastModifiedBy = "SYSTEM";
		lastModifiedDateTime = java.time.LocalDateTime.now();
	}

}
